alias tmux="tmux -2"
alias tmuxa="tmux -2 attach -t"
alias tmuxk="tmux kill-session -t"
alias tmuxl="tmux list-session" 

## HINT: to see all tmux session (not owned), use `pgrep tmux`, and `/proc/$ID/exe attach`

