## tmux
alias tmux="tmux -2"
alias tmuxa="tmux -2 attach -t"
alias tmuxk="tmux kill-session -t"
alias tmuxl="tmux list-session" 

## applications
alias of240='source $HOME/opt/src/OpenFOAM/OpenFOAM-2.4.0/etc/bashrc WM_NCOMPPROCS=32 foamCompiler=ThirdParty WM_COMPILER=Gcc48 WM_MPLIB=SYSTEMOPENMPI; source $HOME/opt/src/OpenFOAM/OpenFOAM-2.4.0/bin/tools/RunFunctions' 
alias fluent15='export PATH=$PATH:$HOME/bin:/usr/local/ansys_inc/v150/fluent/bin:/usr/local/ansys_inc/v150/Framework/bin/Linux64' 
alias blender="blender 2>/dev/null"
alias matlab='matlab -nodesktop -nosplash'

